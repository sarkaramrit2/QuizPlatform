<script>

function check(){
	var file = document.getElementById("file").value;
	/* alert(file); */
	if(file.lastIndexOf("csv")===file.length-3)
		addError("Select a File");
		return false;
	}
	//window.location.replace("/admin/tagAdmin");
	return true;
}

</script>

<div class="col-xs-3">
	<form method="POST" enctype="multipart/form-data" action="/admin/tagAdmin/uploadFile/">
		<input class="btn btn-primary" type="file" name="file" id="file"><!-- <br><input type="text"  class="form-control" name="url" id="url"> -->
		<br><input class="btn btn-primary" type="submit"
			value="Upload" onclick="return check()">
	</form>
</div>