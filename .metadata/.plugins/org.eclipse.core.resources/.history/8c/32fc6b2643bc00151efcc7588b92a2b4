$("#campaignQueryMapping-submit").click(function() {
	var queryIds = "";
	$(".querySelect").each(function( index ) {
		console.log( index + ": " + $( this ).text() );
		if(queryIds == ""){
			queryIds = $(this).attr("queryId");
		}else{
			queryIds = queryIds + "," + $(this).attr("queryId");
		}
	});
	
	//queryIds.substring(0, queryIds.length - 1);
	console.log(queryIds);
	var ajaxUrl = '/admin/landingPage/submitCampaignQueryMappings?campaignId=' + campaignId + "&queryIds=" + queryIds;
	$.ajax({url:ajaxUrl, type: "GET", dataType: 'json', success: function(data) {
		var status = data['status'];
		var msg = data['message'];
		if (status == "success") {
			resetAll();
			addSuccessMessage(msg);
		} else {
			addError(msg);
		}
	}});
});	

$("#campaignQueryMapping-reset").click(function() {
	removeCampaign(campaignId);
});