package com.quiz.utils;

public enum SystemProperty {

	SOLR_MASTER_SERVER("solr.server.url","http://localhost:8983/solr");
	
	private String name;
	private String value;
	
	private SystemProperty(String name, String value){  
		this.name = name;
		this.value=value;  
	}
	
	public String getProperty(){
		return this.value;
	}
	
	
	
}
